{
  "name": "revert-keeper-scripts",
  "version": "0.0.1",
  "description": "Wonderland Compound keeper scripts for The Keep3r Network",
  "keywords": [
    "keep3r",
    "job",
    "ethereum",
    "maker"
  ],
  "license": "MIT",
  "author": "Wonderland",
  "contributors": [
    "Ashitaka (https://github.com/ashitakah)",
    "wei3erHase (https://github.com/wei3erHase)"
  ],
  "private": true,
  "scripts": {
    "postinstall": "eth-sdk",
    "start": "ts-node src/shared/test.ts",
    "build": "eth-sdk && tsc",
    "prod": "node dist/src/upkeep-job.js",
    "lint": "xo",
    "lint:fix": "xo --fix"
  },
  "lint-staged": {
    "*.ts": "yarn lint"
  },
  "dependencies": {
    "dotenv": "16.0.3",
    "ethers": "5.6.9"
  },
  "devDependencies": {
    "@keep3r-network/keeper-scripting-utils": "0.0.0-a0ca2299",
    "@dethcrypto/eth-sdk": "0.3.3",
    "@dethcrypto/eth-sdk-client": "0.1.6",
    "@types/lodash.isequal": "4.5.6",
    "husky": ">=6",
    "lint-staged": ">=10",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "xo": "0.52.3"
  },
  "xo": {
    "prettier": true,
    "rules": {
      "unicorn/prefer-module": 0,
      "unicorn/no-new-array": "warn",
      "import/extensions": 0,
      "new-cap": 0,
      "@typescript-eslint/no-unsafe-assignment": 0,
      "@typescript-eslint/no-dynamic-delete": "warn",
      "@typescript-eslint/no-unsafe-call": "warn",
      "@typescript-eslint/no-unsafe-return": "warn",
      "@typescript-eslint/naming-convention": [
        "error",
        {
          "selector": "variable",
          "format": [
            "camelCase",
            "UPPER_CASE"
          ]
        }
      ]
    }
  }
}
